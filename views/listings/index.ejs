<% layout("layouts/boilerplate")%>

    <h1>ALl listings</h1>
    <h3>Titles
    </h3>
    
<div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1">
  
   <% for(let list of alllisting){ %>
<a href="/listing/<%= list._id%>"  class="listing-links" >
    <div class="card" >
       <!-- <li  ><a href=" /listing/<%=list._id%>"> <%=list.title%></a>--> 
        <img src="<%=list.image.url%>" class="card-img-top" alt="listing image" style="height: 20rem;">
        <div class="card-img-overlay">
         <!-- <h5 class="card-title">Card title</h5>
          <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
          <p class="card-text"><small>Last updated 3 mins ago</small></p> --> 
        </div>
        <div class="card-body">
          <p class="card-text">
           <b> <%=list.title%> </b> 
          
             <br>
             Price: &#8360; <%= list.price ? list.price.toLocaleString("en-pk") : "N/A" %>

          </p>
        </div>
      </div>
    </a>
    <%}%>
</div>
    


